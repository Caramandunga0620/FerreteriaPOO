package ferreteria;

public class FerreteriaCliente {
    private String nombre;
    private String dni;

    public FerreteriaCliente(String nombre, String dni) {
        this.nombre = nombre;
        this.dni = dni;
    }

    public String getNombre() {
        return nombre;
    }

    public String getDni() {
        return dni;
    }

    public void comprar() {
        System.out.println(nombre + " est√° realizando una compra.");
    }
}
 